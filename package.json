    "cypress-xpath": "^2.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "allure-commandline": "^2.34.1",
    "cypress-plugin-api": "^2.11.2",
    "cypress-real-events": "^1.14.0",
    "ip": "^1.1.8",
    "pdfkit": "^0.15.2"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": false
    },
    "stepDefinitions": [

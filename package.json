  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "cypress": "^14.5.4",
    "cypress-junit-reporter": "^1.3.1",
